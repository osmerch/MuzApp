<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MuzApp.Reg.RegPage2">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Укажите свою дату рождения" VerticalOptions="Center" Margin="0, 0, 0, 0"   HorizontalOptions="Center" FontAttributes="Bold" FontSize="24" TextColor="Black"></Label>
            <DatePicker x:Name="DateOfBirthPic" ></DatePicker>
            <Button TextColor="White" Margin="20, 20, 0, 0"   BackgroundColor="#4f3129" CornerRadius="50" WidthRequest="200" Text="Далее" FontSize="13" Clicked="Reg2_Clicked"  x:Name="Reg2"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>